{
  "expo": {
    "name": "Marvel App",
    "slug": "marvel-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/marvelapp.png",
    "scheme": "marvel-app",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#121212"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/marvelApp.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jardel.marvelApp"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
